{
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": true,
    "lib": ["ESNext"],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "outDir": "./dist",
    "paths": {
      "~/*": ["./src/*"]
    },
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2022",
    "types": ["@cloudflare/workers-types", "node"]
  },
  "include": ["./src"]
}
