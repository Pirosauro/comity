{
  "name": "@comity/graphql",
  "version": "0.4.7",
  "type": "module",
  "author": "Filippo Bovo <hello@filippobovo.com>",
  "license": "MIT",
  "keywords": [
    "hono",
    "cloudflare",
    "graphql"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pirosauro/comity.git"
  },
  "scripts": {
    "build": "tsc",
    "prepublish": "tsc",
    "clean": "tsc --build --clean",
    "test": "vitest run --coverage"
  },
  "files": [
    "./dist",
    "./types"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "types": [
        "./types"
      ]
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "dependencies": {
    "@envelop/core": "^5.2.3",
    "hono": "^4.7.10"
  },
  "devDependencies": {
    "@hono/vite-dev-server": "^0.19.1",
    "@types/node": "^22.15.21",
    "@vitest/coverage-v8": "^3.1.4",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  }
}
